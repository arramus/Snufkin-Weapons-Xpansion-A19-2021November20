		</lootgroup>
		<lootgroup name="RedLoot" loot_quality_template="default_qualityT">
			<item name="CrossbowMag" prob=".17"/>
			<item name="HyperBlaster Ticket" prob=".17"/>
			<item name="Railgun Ticket" prob=".17"/>
			<item name="Vampire Gauntlets" prob=".17"/>
			<item name="DCLXVI Zeus" prob=".17"/>
			<item name="Black Widow" prob=".17"/>			
		</lootgroup>
		<lootgroup name="PurpleLoot" loot_quality_template="default_qualityT">
			<item name="Gamma Gun Ticket" prob=".2"/>
			<item name="Kronos XII" prob=".2"/>
			<item name="Vulcan" prob=".2"/>
			<item name="CrimsonHunter" prob=".2"/>
